{
    "palette": {
        "butter1": "#fce94f",
        "butter2": "#edd400",
        "butter3": "#c4a000",
        "chameleon1": "#8ae234",
        "chameleon2": "#73d216",
        "chameleon3": "#4e9a06",
        "orange1": "#fcaf3e",
        "orange2": "#f57900",
        "orange3": "#ce5c00",
        "skyblue1": "#729fcf",
        "skyblue2": "#3465a4",
        "skyblue3": "#204a87",
        "plum1": "#ad7fa8",
        "plum2": "#75507b",
        "plum3": "#5c3566",
        "chocolate1": "#e9b96e",
        "chocolate2": "#c17d11",
        "chocolate3": "#8f5902",
        "scarletred1": "#ef2929",
        "scarletred2": "#cc0000",
        "scarletred3": "#a40000",
        "aluminium1": "#eeeeec",
        "aluminium2": "#d3d7cf",
        "aluminium3": "#babdb6",
        "aluminium4": "#888a85",
        "aluminium5": "#555753",
        "aluminium6": "#2e3436",
        "aluminium7": "#282d2f",
        "white": "#ffffff",
        "aluminium4-3-blend": "#a1a49e",
        "aluminium6-5-blend": "#34393A"
    },
    "style": {
        "text": {
            "foreground": "aluminium2",
            "background": "aluminium6"
        },
        "selection": {
            "foreground": "aluminium1",
            "background": "aluminium4"
        },
        "outside": {
            "foreground": "aluminium1",
            "background": "aluminium7"
        },
        "cursor": {
            "foreground": "aluminium2"
        },
        "secondary-cursor": {
            "foreground": "aluminium4-3-blend"
        },
        "current-line": {
            "background": "aluminium5"
        },
        "line-numbers": {
            "foreground": "aluminium5",
            "background": "#black"
        },
        "draw-spaces": {
            "foreground": "aluminium4"
        },
        "background-pattern": {
            "background": "aluminium6-5-blend"
        },
        "bracket-match": {
            "foreground": "chocolate2"
        },
        "bracket-mismatch": {
            "foreground": "aluminium1",
            "background": "scarletred2"
        },
        "right-margin": {
            "foreground": "aluminium1",
            "background": "aluminium3"
        },
        "search-match": {
            "foreground": "aluminium1",
            "background": "chameleon3"
        },
        "def:comment": {
            "foreground": "aluminium4"
        },
        "def:shebang": {
            "foreground": "aluminium4",
            "bold": "true"
        },
        "def:doc-comment-element": {
            "italic": "true"
        },
        "def:constant": {
            "foreground": "butter2"
        },
        "def:string": {
            "foreground": "butter2"
        },
        "def:special-char": {
            "foreground": "orange3"
        },
        "def:special-constant": {
            "foreground": "orange3"
        },
        "def:floating-point": {
            "foreground": "orange3"
        },
        "def:identifier": {
            "foreground": "skyblue1"
        },
        "def:statement": {
            "foreground": "white",
            "bold": "true"
        },
        "def:type": {
            "foreground": "chameleon1",
            "bold": "true"
        },
        "def:preprocessor": {
            "foreground": "plum1"
        },
        "def:error": {
            "foreground": "aluminium1",
            "background": "scarletred2",
            "bold": "true"
        },
        "def:warning": {
            "foreground": "aluminium1",
            "background": "plum1"
        },
        "def:note": {
            "background": "butter1",
            "foreground": "aluminium4",
            "bold": "true"
        },
        "def:underlined": {
            "italic": "true",
            "underline": "single"
        },
        "def:heading0": {
            "scale": "5.0"
        },
        "def:heading1": {
            "scale": "2.5"
        },
        "def:heading2": {
            "scale": "2.0"
        },
        "def:heading3": {
            "scale": "1.7"
        },
        "def:heading4": {
            "scale": "1.5"
        },
        "def:heading5": {
            "scale": "1.3"
        },
        "def:heading6": {
            "scale": "1.2"
        },
        "diff:added-line": {
            "foreground": "butter2"
        },
        "diff:removed-line": {
            "foreground": "skyblue1"
        },
        "diff:changed-line": {
            "foreground": "plum1"
        },
        "diff:diff-file": {
            "foreground": "chameleon1",
            "bold": "true"
        },
        "diff:location": {
            "foreground": "chameleon1"
        },
        "diff:special-case": {
            "foreground": "white",
            "bold": "true"
        },
        "latex:command": {
            "foreground": "chameleon1",
            "bold": "true"
        },
        "latex:include": {
            "use-style": "def:preprocessor"
        }
    }
}
